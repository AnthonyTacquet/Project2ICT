<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Empty" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/cyan</item>
        <item name="colorPrimaryVariant">@color/darkcyan</item>
        <item name="colorOnPrimary">@color/lightcyan</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/bl</item>
        <item name="colorSecondaryVariant">@color/darkcyan</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
        <item name="android:windowBackground">@color/lightcyan</item>
    </style>

    <style name="Theme.Empty.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.Empty.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.Empty.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>